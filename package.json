{
  "name": "rag-chatbot-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "npm run dev & npm run logs:watch",
    "dev:watch-logs": "concurrently \"npm run dev\" \"npm run logs:watch\" --names \"SERVER,LOGS\" --prefix-colors \"blue,green\"",
    "dev:with-logs": "npm run dev & sleep 3 && npm run logs:watch",
    "worker": "node queueWorker.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "setup": "node scripts/initialize.js",
    "test": "jest",
    "lint": "eslint .",
    "swagger:generate": "node scripts/generateSwaggerDocs.js",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:status": "npx sequelize-cli db:seed:status",
    "seed:templates": "npx sequelize-cli db:seed --seed db/seeders/notification-template-seeder.js",
    "seed:users": "npx sequelize-cli db:seed --seed *demo-users.js",
    "seed:specific": "npx sequelize-cli db:seed --seed",
    "logs:beautify": "node scripts/vortex-log-beautifier.js",
    "logs:watch": "tail -f logs/application-$(date +%Y-%m-%d).log | node scripts/vortex-log-beautifier.js /dev/stdin",
    "logs:errors": "node scripts/vortex-log-beautifier.js logs/error-$(date +%Y-%m-%d).log",
    "logs:compact": "node scripts/vortex-log-beautifier.js logs/application-$(date +%Y-%m-%d).log --compact",
    "logs:clean": "node scripts/vortex-log-beautifier.js logs/application-$(date +%Y-%m-%d).log --hide-socket-metrics --hide-server-stats",
    "logs:requests": "node scripts/vortex-log-beautifier.js logs/application-$(date +%Y-%m-%d).log --compact | grep -E 'üåê|üîå|üì®'",
    "logs:performance": "node scripts/vortex-log-beautifier.js logs/application-$(date +%Y-%m-%d).log | grep '‚ö° Performance'",
    "logs:audit": "node scripts/vortex-log-beautifier.js logs/application-$(date +%Y-%m-%d).log | grep 'üîç Audit'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@bull-board/api": "^5.9.1",
    "@bull-board/express": "^5.9.1",
    "@socket.io/redis-adapter": "^8.2.1",
    "apn": "^2.2.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "bull-board": "^2.1.3",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.11.0",
    "flat": "^6.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "openai": "^4.94.0",
    "opossum": "^8.1.3",
    "ora": "^8.2.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.7.1",
    "sequelize": "^6.37.0",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.2",
    "socket.io-redis": "^6.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
